        "ts-jest": "^28.0.7",
        "ts-node-dev": "^1.1.6",
        "tsc": "^2.0.3",
        "typescript": "4.6.4"
    },
    "dependencies": {
        "@atz3n/kraken-invest-lib": "1.3.2",
        "axios": "^1.1.3",
