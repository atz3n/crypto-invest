        "ts-node-dev": "^1.1.6",
        "tsc": "^2.0.3",
        "typescript": "4.6.4"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "lib": "../lib",
        "mongoose": "^6.5.0",
